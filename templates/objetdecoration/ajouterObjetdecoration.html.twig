{% extends 'base.html.twig' %}

{% block title %}Ajouter un Objet Décoratif{% endblock %}

{% block body %}
  <div class="container">
    <h1 class="text-center mb-5">Ajouter un Objet Décoratif</h1>

    {{ form_start(form) }}
      {{ form_row(form.nom) }}
      {{ form_row(form.description) }}
      {{ form_row(form.prix) }}
      {{ form_row(form.image) }}
      <button class="btn btn-primary" type="submit">Enregistrer</button>
    {{ form_end(form) }}
  </div>
{% endblock %}
